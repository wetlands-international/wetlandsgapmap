{
  "name": "wetlands",
  "version": "1.0.0",
  "description": "A blank template to get started with Payload 3.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev --turbo",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev --turbo",
    "build": "cross-env NODE_OPTIONS=--no-deprecation next build",
    "ci": "payload migrate && pnpm build",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "test": "cross-env NODE_OPTIONS=--no-deprecation vitest",
    "check-types": "cross-env NODE_OPTIONS=--no-deprecation tsc",
    "openapi": "node ./src/openapi.js && pnpm lint --fix",
    "prepare": "husky",
    "heroku-cleanup": "rm -rf .next/cache && find node_modules/@next -depth -maxdepth 1 -type d -name 'swc*' -exec rm -rf {} \\; -prune #Remove cache and SWC binaries to reduce heroku slug size"
  },
  "dependencies": {
    "@dnd-kit/core": "6.3.1",
    "@dnd-kit/modifiers": "9.0.0",
    "@dnd-kit/sortable": "10.0.0",
    "@dnd-kit/utilities": "3.2.2",
    "@payloadcms/db-postgres": "3.37.0",
    "@payloadcms/next": "3.37.0",
    "@payloadcms/payload-cloud": "3.37.0",
    "@payloadcms/richtext-lexical": "3.37.0",
    "@payloadcms/ui": "3.37.0",
    "@radix-ui/react-accordion": "^1.2.10",
    "@radix-ui/react-dialog": "^1.1.7",
    "@radix-ui/react-dropdown-menu": "^2.1.12",
    "@radix-ui/react-popover": "^1.1.13",
    "@radix-ui/react-separator": "^1.1.3",
    "@radix-ui/react-slider": "^1.2.4",
    "@radix-ui/react-slot": "^1.2.0",
    "@radix-ui/react-switch": "^1.1.4",
    "@radix-ui/react-tooltip": "^1.2.0",
    "@react-three/drei": "^10.0.7",
    "@react-three/fiber": "^9.1.2",
    "@t3-oss/env-nextjs": "0.12.0",
    "@tailwindcss/postcss": "4.1.3",
    "@tanstack/react-query": "5.75.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cross-env": "^7.0.3",
    "graphql": "^16.8.1",
    "jotai": "2.12.3",
    "lucide-react": "^0.488.0",
    "mapbox-gl": "3.11.1",
    "next": "15.3.2",
    "next-intl": "^4.1.0",
    "nuqs": "2.4.1",
    "openapi-fetch": "0.13.7",
    "openapi-react-query": "0.4.1",
    "payload": "3.37.0",
    "postcss": "8.5.3",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-icons": "5.5.0",
    "react-map-gl": "8.0.4",
    "sharp": "0.32.6",
    "slugify": "1.6.6",
    "tailwind-merge": "^3.2.0",
    "tailwindcss": "4.1.3",
    "three": "^0.176.0",
    "usehooks-ts": "3.1.1",
    "zod": "3.24.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@next/eslint-plugin-next": "15.3.0",
    "@tailwindcss/typography": "0.5.16",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.3.0",
    "@types/mapbox-gl": "3.4.1",
    "@types/node": "^22.5.4",
    "@types/react": "19.0.12",
    "@types/react-dom": "19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.16.0",
    "eslint-config-next": "15.2.3",
    "eslint-config-prettier": "10.1.1",
    "eslint-plugin-prettier": "5.2.6",
    "eslint-plugin-react-hooks": "5.2.0",
    "husky": "^9.1.7",
    "jsdom": "^26.0.0",
    "openapi-typescript": "7.7.1",
    "payload-oapi": "0.2.0",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "0.6.11",
    "tw-animate-css": "^1.2.5",
    "typescript": "5.7.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.1.1"
  },
  "packageManager": "pnpm@10.10.0"
}
